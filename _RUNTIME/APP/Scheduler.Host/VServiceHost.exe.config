<configuration>
  <configSections>
    <section name="nlog" type="NLog.Config.ConfigSectionHandler, NLog" />
    <!-- For more information on Entity Framework configuration, visit http://go.microsoft.com/fwlink/?LinkID=237468 -->
    <section name="entityFramework" type="System.Data.Entity.Internal.ConfigFile.EntityFrameworkSection, EntityFramework, Version=6.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089" requirePermission="false" />
    <section name="vsoft.hosting" type="VSoft.GenericServiceHost.Configuration.HostingSettings, VSoft.GenericServiceHost" />
    <section name="hibernate-configuration" type="NHibernate.Cfg.ConfigurationSectionHandler, NHibernate" />
    <section name="vsoft.genericServiceHost.fileTriggers" type="VSoft.GenericServiceHost.FileTriggers.Configuration.FileTriggersSettings, VSoft.GenericServiceHost.FileTriggers" />
    <section name="vsoft.framework.log" type="VSoft.Framework.Log.Configuration.XmlConfigurator, VSoft.Framework.Log" />
    <!--
    Baza danych ORACLE
    <section name="oracle.manageddataaccess.client" type="OracleInternal.Common.ODPMSectionHandler, Oracle.ManagedDataAccess, Version=4.121.2.0, Culture=neutral, PublicKeyToken=89b483f429c47342" />
    -->
  </configSections>
  <nlog>
    <include file="${basedir}/Configuration/Environment/NLog.config" />
  </nlog>
  <system.data>
    <DbProviderFactories>
      <remove invariant="Oracle.ManagedDataAccess.Client" />
      <add name="ODP.NET, Managed Driver" invariant="Oracle.ManagedDataAccess.Client" description="Oracle Data Provider for .NET, Managed Driver" type="Oracle.ManagedDataAccess.Client.OracleClientFactory, Oracle.ManagedDataAccess, Version=4.121.2.0, Culture=neutral, PublicKeyToken=89b483f429c47342" />
      <remove invariant="Npgsql.NpgsqlFactory" />
      <add name="Npgsql.NpgsqlFactory" invariant="Npgsql.NpgsqlFactory" type="Npgsql.NpgsqlFactory, Npgsql" description=".NET Framework Data Provider for Npgsql.NpgsqlFactory" />
      <remove invariant="MySql.Data.MySqlClient" />
      <add name="MySql.Data.MySqlClient" invariant="MySql.Data.MySqlClient" type="MySql.Data.MySqlClient.MySqlClientFactory, MySql.Data" description=".NET Framework Data Provider for MySql" />
    </DbProviderFactories>
  </system.data>
  <appSettings file="Configuration\Environment\AppSettings.config" />
  <connectionStrings configSource="Configuration\Environment\ConnectionStrings.config" />
  <vsoft.framework.log configSource="Configuration\Environment\Logger.config" />
  <vsoft.hosting>
    <startFirst>
      <add name="InterCommModule" order="1" />
      <add name="TaskTrackingModule" order="2" />
      <add name="ChangeTrackingModule" order="2" />
      <add name="NotificationModule" order="3" />
      <add name="TMModule" order="3" />
      <add name="CodeTasksModule" order="3" />
      <add name="FileTriggersModule" order="4" />
      <add name="ExternalProcessLauncherModule" order="5" />
      <add name="CrossDomainDiagnosticModule" order="6" />
      <add name="RestApiModule" order="7" />
    </startFirst>
  </vsoft.hosting>
  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.8" />
  </startup>
  <hibernate-configuration xmlns="urn:nhibernate-configuration-2.2">
    <!--
    Konfiguracja dla bazy danych Oracle, dla sterownika OracleManaged
    <session-factory>
      <property name="connection.driver_class">NHibernate.Driver.OracleManagedDataClientDriver</property>
      <property name="connection.connection_string_name">UH</property>
      <property name="show_sql">true</property>
      <property name="dialect">NHibernate.Dialect.Oracle10gDialect</property>
      <property name="query.substitutions">true 1, false 0, yes 'Y', no 'N'</property>
    </session-factory>
    -->
    <session-factory>
      <property name="dialect">NHibernate.Dialect.MsSql2005Dialect</property>
      <property name="connection.provider">NHibernate.Connection.DriverConnectionProvider</property>
      <property name="connection.driver_class">NHibernate.Driver.SqlClientDriver</property>
      <property name="connection.connection_string_name">UH</property>
      <!--<property name="hbm2ddl.auto">create</property>-->
      <!-- Pokazywanie sql-i generowanych przez NHibernate:-->
      <!--<property name="show_sql">true</property>-->
    </session-factory>
  </hibernate-configuration>
